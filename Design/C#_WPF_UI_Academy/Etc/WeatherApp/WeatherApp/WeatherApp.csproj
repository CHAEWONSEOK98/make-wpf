    <None Remove="Images\rain.png" />
    <None Remove="Images\sun.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\city.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
